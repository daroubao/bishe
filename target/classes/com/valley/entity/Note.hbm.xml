<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.valley.entity.Note" table="note">
		<id name="id" column="Id">
			<generator class="native" />
		</id>
		<property name="noteID" column="NoteID" />
		<property name="noteTitle" column="NoteTitle" />
		<property name="noteDesc" column="NoteDesc" />
		<property name="noteDate" column="NoteDate" />
		<property name="noteUserID" column="NoteUserID" />
		
	</class>
</hibernate-mapping>
