<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.valley.entity.Album" table="album">
		<id name="id" column="Id">
			<generator class="native" />
		</id>
		<property name="albumID" column="AlbumID" />
		<property name="albGroupNa" column="AlbGroupNa" />
		<property name="albUserID" column="AlbUserID" />
		<property name="albBouUserID" column="AlbBouUserID" />
		<property name="place" column="Place" />
		<property name="albDate" column="AlbDate" />
		<property name="albDesc" column="AlbDesc" />
		<property name="albRights" column="AlbRights" />
	</class>
</hibernate-mapping>
