<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.valley.entity.Festival" table="festival">
		<id name="id" column="Id">
			<generator class="native" />
		</id>
		<property name="fesID" column="FesID" />
		<property name="fesTitle" column="FesTitle" />
		<property name="fesDate" column="FesDate" />
		<property name="remindDate" column="RemindDate" />
		<property name="fesDesc" column="FesDesc" />
		<property name="fesUserID" column="FesUserID" />
		
	</class>
</hibernate-mapping>
